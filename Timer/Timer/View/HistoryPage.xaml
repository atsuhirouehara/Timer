<Page x:Class="Timer.View.HistoryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Timer.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" Background="#FFF0F0F0"
      Title="HistoryPage">

    <Grid>
        <DockPanel Margin="0,0,-56,0">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Menu">
                    <MenuItem Header="トップページ" Click="TopPage_Click" />
                    <MenuItem Header="比較ページ" Click="ComparePage_Click"/>
                </MenuItem>
            </Menu>
        </DockPanel>
        
        
    </Grid>
</Page>
